{{define "admin.controllers.module" -}}
{{- $ctx := . -}}

package {{index $ctx.Properties.Build "package"}}

import (
	"github.com/goatcms/goatcore/app"
	{{- range $index, $key := (keys .Data "model.") }}
	{{$key}} "{{index $ctx.Properties.Build "path"}}/model/{{$key}}"
	{{- end -}}
)

func InitDependencies(a app.App) error {
	{{- range $index, $key := (keys .Data "model.") }}
	if err := {{$key}}.InitDependencies(a); err != nil {
		return err
	}
	{{- end }}
	return nil
}


{{- end -}}
