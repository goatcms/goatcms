{{- $ctx := . -}}

{{- range $index, $key := (keys .Data "model.") -}}

  {{- /* Insert */ -}}
  {{- $listPath := (print $ctx.To "/model/" $key "/list.go") -}}
  {{- if not ($ctx.Filesystem.IsFile $listPath) -}}
    {{- $ctx.Out.File $listPath -}}
      {{- template "controller.model.list" (ctx $ctx (print "model." $key) "") -}}
    {{- $ctx.Out.EOF -}}
  {{- end -}}

{{- end -}}
