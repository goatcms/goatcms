{{- $ctx := . -}}

{{- range $index, $key := (keys .Data "model.") -}}

  {{- /* List */ -}}
  {{- $listPath := (print $ctx.To "/model/" $key "/list/main.gohtml") -}}
  {{- if not ($ctx.Filesystem.IsFile $listPath) -}}
    {{- $ctx.Out.File $listPath -}}
      {{- template "admin.views.model.list" (ctx $ctx (print "model." $key) "") -}}
    {{- $ctx.Out.EOF -}}
  {{- end -}}

  {{- /* Insert */ -}}
  {{- $insertPath := (print $ctx.To "/model/" $key "/insert/main.gohtml") -}}
  {{- if not ($ctx.Filesystem.IsFile $insertPath) -}}
    {{- $ctx.Out.File $insertPath -}}
      {{- template "admin.views.model.insert" (ctx $ctx (print "model." $key) "") -}}
    {{- $ctx.Out.EOF -}}
  {{- end -}}
{{- end -}}
