[{
  "to":"config",
  "layout":"default",
  "template":"config",
},{
  "to":"cmsapp/entities",
  "layout":"default",
  "template":"entity",
  "properties": {
    "package":"entities"
  },
  "afterBuild": "gofmt -w {{argument.cwd}}cmsapp/entities/",
},{
  "to":"cmsapp/dao",
  "layout":"default",
  "template":"dao",
  "properties": {
    "package":"dao",
    "path":"github.com/goatcms/goatcms/cmsapp/dao",
    "entities_path":"github.com/goatcms/goatcms/cmsapp/entities"
  },
  "afterBuild": "gofmt -w {{argument.cwd}}cmsapp/dao/sqlite/",
},{
  "to":"cmsapp/controllers/admin",
  "layout":"default",
  "template":"admin/controllers",
  "properties": {
    "package":"adminctrl",
    "path":"github.com/goatcms/goatcms/cmsapp/controller/admin",
    "entities_path":"github.com/goatcms/goatcms/cmsapp/entities"
  },
  "afterBuild": "gofmt -w {{argument.cwd}}cmsapp/controller/admin/",
}]
