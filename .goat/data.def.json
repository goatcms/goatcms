[{
  "type":"model",
  "properties":[{
    "prompt":"insert single entity name",
    "key":"entity",
    "type":"line",
    "min":1,
    "max":30,
    "pattern": "^[a-zA-z]+[a-zA-Z0-9]*$"
  }],
  "collections":[{
    "key": "fields",
    "type": "array",
    "prompt": "Insert entity fields",
    "properties": [{
      "prompt":"Insert field name",
      "key":"name",
      "type":"line",
      "min":1,
      "max":25,
      "pattern": "^[A-Za-z]+$"
    },{
      "prompt":"Insert field type",
      "key":"type",
      "type":"line",
      "min":1,
      "max":25,
      "pattern": "^(string|text|int)$"
    },{
      "prompt":"The field is unique? (y/n)",
      "key":"unique",
      "type":"line",
      "min":1,
      "max":4,
      "pattern": "^(y|n)$"
    }]
  }]
},{
  "type":"query",
  "properties":[{
    "prompt":"query name",
    "key":"name",
    "type":"line",
    "min":1,
    "max":30,
    "pattern": "^[a-zA-z]+[a-zA-Z0-9]*$"
  },{
    "prompt":"related model",
    "key":"model",
    "type":"line",
    "min":1,
    "max":30,
    "pattern": "^[a-zA-z]+[a-zA-Z0-9]*$"
  },{
    "prompt":"is it a single result query?",
    "key":"isSingleResult",
    "type":"line",
    "min":1,
    "max":1,
    "pattern": "^(y|n)$"
  }],
  "collections":[{
    "key": "fit",
    "type": "array",
    "prompt": "mathed fields (insert fields separated with spaces)",
    "properties": [{
      "prompt":"OR (field1 AND field2 AND field3 AND ...)",
      "key":"fields",
      "type":"line",
      "min":1,
      "max":2000,
      "pattern": "^[A-Za-z]+[A-Za-z\s]*$"
    }]
  }]
},{
  "type":"lang",
  "collections":[{
    "type": "map",
    "prompt": "Insert translates",
    "properties": [{
      "prompt":"Insert translate",
      "key":"text",
      "type":"line",
      "min":1,
      "max":1024
    }]
  }]
}]
