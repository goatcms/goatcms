{{- define "content" -}}

<h1>Article</h1>

{{ if eq (len .Data.Collection) 0 -}}
No data to show
{{- else }}
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Content</th>
      </tr>
    </thead>
    <tbody>{{- range $i, $article := .Data.Collection }}" }}
      <tr>
        <td>{{- $article.Title -}}</td>
        <td>{{- $article.Content -}}</td>
      <tr>
    {{- end }}
  </tbody>
</table>
{{- end }}


{{- end }}